POTD-#14
TASK-1-DI STRING MATCH
SOLUTION-
  class Solution {
public:
    vector<int> diStringMatch(string s) {
        int n=s.size(),min=0,max=n;
        vector<int>ans;
        for(int i=0;i<n;i++){
            if(s[i]=='I'){
                ans.push_back(min);
                min++;
            }
            else{
                ans.push_back(max);
                max--;
            }
        }
        ans.push_back(max);
        return ans;
    }
};
